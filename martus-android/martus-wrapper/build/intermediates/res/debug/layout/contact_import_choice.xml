<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactImportChoiceActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dip"
        android:layout_marginRight="20dip"
        android:layout_marginLeft="20dip"
        android:layout_gravity="left"
        android:onClick="addContactUsingAccessToken"
        android:text="@string/add_contact_using_access_token"
        android:id="@+id/addContactUsingAccessTokenButton"
        style="@style/Widget.Button"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dip"
        android:layout_marginRight="20dip"
        android:layout_marginLeft="20dip"
        android:layout_gravity="left"
        android:text="@string/add_contact_from_file"
        android:id="@+id/addContactFromFileLink"
        android:linksClickable="true"
        android:autoLink="all"
        android:clickable="true"
        style="@style/MartusTextViewStyle"
        />

</LinearLayout>
