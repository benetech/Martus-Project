<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape>
            <solid
                android:color="@color/button_disabled_background"    />
            <stroke
                android:width="1dp"
                 />
            <corners
                android:radius="1dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/button_pressed_color" />
            <stroke
                android:width="1dp"
                android:color="@color/button_text_white" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient
                android:startColor="@color/button_gradient_start_color"
                android:endColor="@color/button_gradient_end_color"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="@color/button_stroke_color" />
            <corners
                android:radius="1dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>
